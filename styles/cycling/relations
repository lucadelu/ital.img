# Hiking routes
type=route & route=bicycle {
  set name='${name} (${ref}) (${operator})' | '${name} (${ref})' | '${name} (${operator})' | '${ref} (${operator})' |'${name}' | '${ref}';
  apply { set mkgmap:route='$(mkgmap:route),${name}' | '${name}' }
}

